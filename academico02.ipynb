     "output_type": "stream",
     "text": [
      "Digite um número:  7\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "7! = 5040\n"
     ]
    }
   ],
   "source": [
    "numero = int(input('Digite um número: ')) #usando fator e cotador\n",
    "fatorial = 1\n",
    "contador = 1\n",
    "\n",
    "while contador <= numero: # usando a condicional enquanto\n",
    "    fatorial = fatorial * contador\n",
    "    contador += 1 #contato mais um\n",
    "\n",
    "print('{0}! = {1}'.format(numero, fatorial))"
   ]
  },
